DOCKER_PGADMIN_DEFAULT_EMAIL=admin@example.com
DOCKER_PGADMIN_DEFAULT_PASSWORD=password
DOCKER_DB_HOST=postgres
DOCKER_DB_PORT=5432
DB_DATABASE=laravel
DB_SCHEMA=public
DB_USERNAME=laravel
DB_PASSWORD=secret
