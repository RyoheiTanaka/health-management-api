DOCKER_PHP_MEMORY_LIMIT=256M
DOCKER_PHP_UPLOAD_MAX_FILESIZE=50M
DOCKER_PHP_POST_MAX_SIZE=50M
DOCKER_PHP_MAX_EXECUTION_TIME=120
DOCKER_PHP_DISPLAY_ERRORS=On
DOCKER_PHP_XDEBUG_ENABLED=1
DOCKER_PHP_XDEBUG_MODE=debug,develop
DOCKER_PHP_XDEBUG_START_WITH_REQUEST=yes
